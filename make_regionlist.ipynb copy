    "test_range = [8,10]\n",
    "contig = [5,13]\n",
    "xs,ys = unzip(list(zip(test_range,[1]*len(test_range))))\n",
    "plt.plot(xs,ys)\n",
    "xs,ys = unzip(list(zip(contig,[2]*len(contig))))\n",
    "plt.plot(xs,ys)\n",
    "\n",
    "for res in range_takeout(contig, test_range):\n",
    "    xs,ys = unzip(list(zip(res,[0]*len(res))))\n",
    "    plt.plot(xs,ys)"
   ]
