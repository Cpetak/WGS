   "source": [
    "#make it into a dictionary \n",
    "ranges = { interval : defaultdict(list) for interval in intervals }\n",
    "for Var in intervals:\n",
    "    temp_df = al[al[\"type\"]==Var]\n",
    "    for index, row in temp_df.iterrows():\n",
    "        ranges[Var][row['chr']].append([row['start'],row['stop']])"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "metadata": {},
   "outputs": [],
   "source": [
    "def range_takeout(contig, evil):\n",
    "    A0, A1 = contig #region that we like, e.g. conserved region, start and end respectively, start being the lower number no matter the direction of the gene for example\n",
    "    B0, B1 = evil #region that we don't want in the end, e.g. coding region, start and end\n",
    "    if( A1 < B0 or B1 < A0 ): # no overlap\n",
    "        return [contig]\n",
    "    elif ( A0 < B0 and B0 <= A1 and A1 <= B1) : # tail left\n",
    "        return [[A0, B0]]\n",
    "    elif (B0 <= A0 and A0 <= B1 and B1 < A1): # tail right\n",
    "        return [[B1, A1]]\n",
    "    elif ( B0 <= A0 and A1 <= B1): # superset\n",
    "        return []\n",
    "    elif (A0 < B0 and B1 < A1): # subset\n",
    "        return [[A0,B0],[B1,A1]]\n",
    "    else:\n",
    "        print(\"ERROR:\",contig, evil)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 9,
   "metadata": {},
   "outputs": [],
   "source": [
    "def unzip(l):\n",
    "    return list(zip(*l))"
   ]
